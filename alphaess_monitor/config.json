{
    "name": "Alpha ESS to MQTT",
    "version": "0.1.3",
    "slug": "alphaess_monitor",
    "description": "Alpha ESS to MQTT add-on by MarioCake",
    "url": "https://github.com/DasLetzteEinhorn/AlphaESS_Monitor_Hass",
    "startup": "application",
    "arch": [
        "aarch64",
        "amd64",
        "armhf",
        "armv7",
        "i386"
    ],
    "boot": "auto",
    "hassio_api": true,
    "homeassistant_api": true,
    "hassio_role": "default",
    "services": [
        "mqtt:need"
    ],
    "options": {
        "log_level": "info",
        "base_topic": "alphaess",
        "timeout": 60,
        "username": "",
        "password": ""
    },
    "schema": {
        "log_level": "list(trace|debug|info|notice|warning|error|fatal)",
        "username": "str",
        "password": "password",
        "serial_numbers": "str?",
        "base_topic": "str",
        "timeout": "int",
        "mqtt_host": "str?",
        "mqtt_port": "int?",
        "mqtt_username": "str?",
        "mqtt_password": "password?"
    }
}